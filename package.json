    "@rollup/plugin-json": "^4.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.0.1",
    "js-yaml": "^3.14.0",
